@using Wimi.BtlCore.Web.Models.Common.Modals
@await Html.PartialAsync("~/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(L("Compare")))
<div class="modal-body">
    <div class="nav-tabs-custom">
        <ul class="nav nav-tabs">
            <li class="active">
                <a href="#tab_20-20" data-toggle="tab">@L("StatusRecords")</a>
            </li>
            <li>
                <a href="#tab_10-10" data-toggle="tab">@L("UtilizationRatio")</a>
            </li>
        </ul>

        <div class="tab-content">
            <div class="tab-pane active" id="tab_20-20">
                <div class="gantt-timeline"></div>
            </div>

            <div class="tab-pane" id="tab_10-10">
                <div id="chart-line" style="height: 300px"></div>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-primary close-button" data-dismiss="modal">@L("Cancel")</button>
</div>

